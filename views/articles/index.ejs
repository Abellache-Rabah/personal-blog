    <h1>Abellache Rabah</h1>
    <ul>
        
        <li>
            <a href="/">About</a>
        </li>
        
        <li>
            <a href="https://github.com/rabah01abellache">Github</a>
        </li>
        
        <li>
            <a href="https://www.facebook.com/profile.php?id=100039576731201">Facebook</a>
        </li>
        
    </ul>
</div>
<div class="blog">
    <h1>Blog Posts:</h1>
    <ul>
        <% articles.forEach(article => { %>
        <li>
            <span class="date"><%= article.date %></span> - <a href="/articles/<%= article.slug %>"><%= article.title %></a>
        </li>
       <% })%>
    </ul>
</div>

    </div>

</body>

</html>
















